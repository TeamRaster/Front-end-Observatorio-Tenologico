extends templates/indexT

block main
  #fh5co-page
    header#fh5co-header(role='banner')
      .container
        .header-inner
          h1
            i.sl-icon-mustache
            a(href='#') Observatorio
          nav(role='navigation')
            ul
              li: a.active(href='#') Inicio
              li: a(href='#') Oferta y Demanda
              li: a(href='#') Fondos de Apoyo
              li: a(href='#') I+D+I
              li: a(href='#') Informacion tecnologica
    section#home
      .container
        .row
          .col-md-12.col-sm-12
            h3 Observatorio Tecnologico del Estado de Hidalgo
            h1 UAEH
            hr
            a.smoothScroll.btn.btn-singin(href='#work') Iniciar Sesion
            a.smoothScroll.btn.btn-default(href='#contact') Registrarse

    #gtco-features
      .gtco-container
        .row
          .col-md-4.col-sm-4
            .feature-center.animate-box(data-animate-effect='fadeIn')
              span.icon
                i.icon-eye
              h3 Retina Ready
              p
                | Dignissimos asperiores vitae velit veniam totam fuga molestias accusamus alias autem provident. Odit ab aliquam dolor eius.
              p
                a.btn.btn-primary(href='#') Learn More
          .col-md-4.col-sm-4
            .feature-center.animate-box(data-animate-effect='fadeIn')
              span.icon
                i.icon-command
              h3 Fully Responsive
              p
                | Dignissimos asperiores vitae velit veniam totam fuga molestias accusamus alias autem provident. Odit ab aliquam dolor eius.
              p
                a.btn.btn-primary(href='#') Learn More
          .col-md-4.col-sm-4
            .feature-center.animate-box(data-animate-effect='fadeIn')
              span.icon
                i.icon-power
              h3 Web Starter
              p
                | Dignissimos asperiores vitae velit veniam totam fuga molestias accusamus alias autem provident. Odit ab aliquam dolor eius.
              p
                a.btn.btn-primary(href='#') Learn More




    #fh5co-footer(role='contentinfo')
      .container
        .row
          .col-md-4.to-animate
            h3.section-title Acerca de Nosotros
            p
              | Es un campo tecnológico virtual que facilita, desarrolla, optimiza la vinculación, comunicación, colaboración entre empresas, instituciones educativas y centros de investigación.
            p.copy-right
              | © 2017 Observatorio Tecnologico del Esatdo de Hidalgo.
              br
              | All Rights Reserved.
              br
              | Designed by Team Raster
          .col-md-4.to-animate
            h3.section-title Nuestra direccion
            ul.contact-info
              li
                i.icon-map-marker
                | 198 West 21th Street, Suite 721 New York NY 10016
              li
                i.icon-phone
                | + 1235 2355 98
              li
                i.icon-envelope
                a(href='#') info@yoursite.com
              li
                i.icon-globe2
                a(href='#') www.yoursite.com
            h3.section-title Contactanos
            ul.social-media
              li
                a.facebook(href='#')
                  i.icon-facebook
              li
                a.twitter(href='#')
                  i.icon-twitter
              li
                a.dribbble(href='#')
                  i.icon-dribbble
              li
                a.github(href='#')
                  i.icon-github-alt
          .col-md-4.to-animate
            h3.section-title Envianos tu opinion
            form.contact-form
              .form-group
                label.sr-only(for='name') Name
                input#name.form-control(type='name', placeholder='Name')
              .form-group
                label.sr-only(for='email') Email
                input#email.form-control(type='email', placeholder='Email')
              .form-group
                label.sr-only(for='message') Message
                textarea#message.form-control(rows='7', placeholder='Message')
              .form-group
                input#btn-submit.btn.btn-send-message.btn-md(type='submit', value='Send Message')



